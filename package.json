{
  "name": "@osnova/webrtc-signal-firebase",
  "version": "0.2.1",
  "private": false,
  "description": "WebRTC signaling with Firebase",
  "main": "dist/osnova-webrtc-signal-firebase.cjs.js",
  "module": "dist/osnova-webrtc-signal-firebase.esm.js",
  "browser": "dist/osnova-webrtc-signal-firebase.umd.js",
  "types": "dist/index.d.ts",
  "author": "Alexander Snov",
  "license": "MIT",
  "scripts": {
    "clean:dist": "yarn rimraf dist",
    "build:types": "tsc -p . --emitDeclarationOnly",
    "build:code": "rollup -c",
    "build": "yarn clean:dist && yarn build:types && yarn build:code",
    "dev": "rollup -c -w",
    "lint": "eslint . --ext .js,.ts",
    "npm:publish": "node scripts/copy-pkg-json.js && node scripts/publish.js",
    "release": "yarn build && npm:publish",
    "_postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase": "^8.2.7",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "np": "^7.4.0",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "firebase": "^8.2.7"
  },
  "dependencies": {}
}